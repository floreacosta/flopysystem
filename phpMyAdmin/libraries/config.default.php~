    $cfg['DefaultFunctions'] =3D array(
        'FUNC_CHAR' =3D&gt; '',
        'FUNC_DATE' =3D&gt; '',
        'FUNC_NUMBER' =3D&gt; '',
        'FUNC_SPATIAL' =3D&gt; 'GeomFromText',
        'FUNC_UUID' =3D&gt; 'UUID',
        'first_timestamp' =3D&gt; 'NOW',
    );
